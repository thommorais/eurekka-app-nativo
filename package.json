		"eslint": "^6.8.0",
		"expo": "~37.0.3",
		"expo-sensors": "~8.1.0",
		"nachos-ui": "^0.2.0-beta.1",
		"polished": "^3.5.1",
		"react": "~16.9.0",
		"react-dom": "~16.9.0",
		"react-native": "https://github.com/expo/react-native/archive/sdk-37.0.0.tar.gz",
		"react-native-gesture-handler": "~1.6.0",
		"react-native-paper": "^3.7.0",
